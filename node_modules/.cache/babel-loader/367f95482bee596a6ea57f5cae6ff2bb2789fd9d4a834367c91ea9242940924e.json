{"ast":null,"code":"/* eslint-disable no-new */\nimport PerfectScrollbar from \"perfect-scrollbar\";\nimport \"perfect-scrollbar/css/perfect-scrollbar.css\";\n// import SidebarShare from \"./SidebarSharePlugin\";\nfunction hasElement(className) {\n  return document.getElementsByClassName(className).length > 0;\n}\nfunction initScrollbar(className) {\n  if (hasElement(className)) {\n    new PerfectScrollbar(`.${className}`);\n  } else {\n    // try to init it later in case this component is loaded async\n    setTimeout(() => {\n      initScrollbar(className);\n    }, 100);\n  }\n}\nimport DashboardNavbar from \"./DashboardNavbar.vue\";\nimport ContentFooter from \"./ContentFooter.vue\";\nimport SidebarFixedToggleButton from \"./SidebarFixedToggleButton.vue\";\nimport { ZoomCenterTransition } from \"vue2-transitions\";\nexport default {\n  components: {\n    DashboardNavbar,\n    ContentFooter,\n    SidebarFixedToggleButton,\n    ZoomCenterTransition\n    // SidebarShare,\n  },\n  data() {\n    return {\n      sidebarBackground: \"vue\" //vue|blue|orange|green|red|primary\n    };\n  },\n  methods: {\n    toggleSidebar() {\n      if (this.$sidebar.showSidebar) {\n        this.$sidebar.displaySidebar(false);\n      }\n    },\n    initScrollbar() {\n      let docClasses = document.body.classList;\n      let isWindows = navigator.platform.startsWith(\"Win\");\n      if (isWindows) {\n        // if we are on windows OS we activate the perfectScrollbar function\n        initScrollbar(\"sidebar\");\n        initScrollbar(\"main-panel\");\n        initScrollbar(\"sidebar-wrapper\");\n        docClasses.add(\"perfect-scrollbar-on\");\n      } else {\n        docClasses.add(\"perfect-scrollbar-off\");\n      }\n    }\n  },\n  mounted() {\n    this.initScrollbar();\n  }\n};","map":{"version":3,"names":["PerfectScrollbar","hasElement","className","document","getElementsByClassName","length","initScrollbar","setTimeout","DashboardNavbar","ContentFooter","SidebarFixedToggleButton","ZoomCenterTransition","components","data","sidebarBackground","methods","toggleSidebar","$sidebar","showSidebar","displaySidebar","docClasses","body","classList","isWindows","navigator","platform","startsWith","add","mounted"],"sources":["src/pages/Layout/DashboardLayout.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\" :class=\"{ 'nav-open': $sidebar.showSidebar }\">\n    <notifications></notifications>\n    <sidebar-fixed-toggle-button />\n    <side-bar\n      :background-color=\"sidebarBackground\"\n      :short-title=\"$t('sidebar.shortTitle')\"\n      :title=\"$t('sidebar.title')\"\n    >\n      <template slot=\"links\">\n        <sidebar-item\n          :link=\"{\n            name: $t('sidebar.dashboard'),\n            icon: 'tim-icons icon-chart-pie-36',\n            path: '/dashboard',\n          }\"\n        >\n        </sidebar-item>\n        <sidebar-item\n          :link=\"{\n            name: $t('sidebar.neighborhood'),\n            icon: 'tim-icons icon-image-02',\n          }\"\n        >\n          <sidebar-item\n            :link=\"{ name: $t('sidebar.list'), path: '/residentials' }\"\n          ></sidebar-item>\n          <sidebar-item\n            :link=\"{ name: $t('sidebar.new'), path: '/residentials/new' }\"\n          ></sidebar-item>\n        </sidebar-item>\n        <sidebar-item\n          :link=\"{\n            name: $t('sidebar.widgets'),\n            icon: 'tim-icons icon-settings',\n            path: '/widgets',\n          }\"\n        ></sidebar-item>\n        <sidebar-item\n          :link=\"{\n            name: $t('sidebar.charts'),\n            icon: 'tim-icons icon-chart-bar-32',\n            path: '/charts',\n          }\"\n        ></sidebar-item>\n        <sidebar-item\n          :link=\"{\n            name: $t('sidebar.calendar'),\n            icon: 'tim-icons icon-time-alarm',\n            path: '/calendar',\n          }\"\n        ></sidebar-item>\n      </template>\n    </side-bar>\n    <!--Share plugin (for demo purposes). You can remove it if don't plan on using it-->\n    <!--    <sidebar-share :background-color.sync=\"sidebarBackground\"> </sidebar-share>-->\n    <div class=\"main-panel\" :data=\"sidebarBackground\">\n      <dashboard-navbar></dashboard-navbar>\n      <router-view name=\"header\"></router-view>\n      <div\n        :class=\"{ content: !$route.meta.hideContent }\"\n        @click=\"toggleSidebar\"\n      >\n        <zoom-center-transition :duration=\"200\" mode=\"out-in\">\n          <!-- your content here -->\n          <router-view></router-view>\n        </zoom-center-transition>\n      </div>\n      <content-footer v-if=\"!$route.meta.hideFooter\"></content-footer>\n    </div>\n  </div>\n</template>\n<script>\n/* eslint-disable no-new */\nimport PerfectScrollbar from \"perfect-scrollbar\";\nimport \"perfect-scrollbar/css/perfect-scrollbar.css\";\n// import SidebarShare from \"./SidebarSharePlugin\";\nfunction hasElement(className) {\n  return document.getElementsByClassName(className).length > 0;\n}\n\nfunction initScrollbar(className) {\n  if (hasElement(className)) {\n    new PerfectScrollbar(`.${className}`);\n  } else {\n    // try to init it later in case this component is loaded async\n    setTimeout(() => {\n      initScrollbar(className);\n    }, 100);\n  }\n}\n\nimport DashboardNavbar from \"./DashboardNavbar.vue\";\nimport ContentFooter from \"./ContentFooter.vue\";\nimport SidebarFixedToggleButton from \"./SidebarFixedToggleButton.vue\";\nimport { ZoomCenterTransition } from \"vue2-transitions\";\n\nexport default {\n  components: {\n    DashboardNavbar,\n    ContentFooter,\n    SidebarFixedToggleButton,\n    ZoomCenterTransition,\n    // SidebarShare,\n  },\n  data() {\n    return {\n      sidebarBackground: \"vue\", //vue|blue|orange|green|red|primary\n    };\n  },\n  methods: {\n    toggleSidebar() {\n      if (this.$sidebar.showSidebar) {\n        this.$sidebar.displaySidebar(false);\n      }\n    },\n    initScrollbar() {\n      let docClasses = document.body.classList;\n      let isWindows = navigator.platform.startsWith(\"Win\");\n      if (isWindows) {\n        // if we are on windows OS we activate the perfectScrollbar function\n        initScrollbar(\"sidebar\");\n        initScrollbar(\"main-panel\");\n        initScrollbar(\"sidebar-wrapper\");\n\n        docClasses.add(\"perfect-scrollbar-on\");\n      } else {\n        docClasses.add(\"perfect-scrollbar-off\");\n      }\n    },\n  },\n  mounted() {\n    this.initScrollbar();\n  },\n};\n</script>\n<style lang=\"scss\">\n$scaleSize: 0.95;\n@keyframes zoomIn95 {\n  from {\n    opacity: 0;\n    transform: scale3d($scaleSize, $scaleSize, $scaleSize);\n  }\n  to {\n    opacity: 1;\n  }\n}\n\n.main-panel .zoomIn {\n  animation-name: zoomIn95;\n}\n\n@keyframes zoomOut95 {\n  from {\n    opacity: 1;\n  }\n  to {\n    opacity: 0;\n    transform: scale3d($scaleSize, $scaleSize, $scaleSize);\n  }\n}\n\n.main-panel .zoomOut {\n  animation-name: zoomOut95;\n}\n</style>\n"],"mappings":"AAyEA;AACA,OAAAA,gBAAA;AACA;AACA;AACA,SAAAC,WAAAC,SAAA;EACA,OAAAC,QAAA,CAAAC,sBAAA,CAAAF,SAAA,EAAAG,MAAA;AACA;AAEA,SAAAC,cAAAJ,SAAA;EACA,IAAAD,UAAA,CAAAC,SAAA;IACA,IAAAF,gBAAA,KAAAE,SAAA;EACA;IACA;IACAK,UAAA;MACAD,aAAA,CAAAJ,SAAA;IACA;EACA;AACA;AAEA,OAAAM,eAAA;AACA,OAAAC,aAAA;AACA,OAAAC,wBAAA;AACA,SAAAC,oBAAA;AAEA;EACAC,UAAA;IACAJ,eAAA;IACAC,aAAA;IACAC,wBAAA;IACAC;IACA;EACA;EACAE,KAAA;IACA;MACAC,iBAAA;IACA;EACA;EACAC,OAAA;IACAC,cAAA;MACA,SAAAC,QAAA,CAAAC,WAAA;QACA,KAAAD,QAAA,CAAAE,cAAA;MACA;IACA;IACAb,cAAA;MACA,IAAAc,UAAA,GAAAjB,QAAA,CAAAkB,IAAA,CAAAC,SAAA;MACA,IAAAC,SAAA,GAAAC,SAAA,CAAAC,QAAA,CAAAC,UAAA;MACA,IAAAH,SAAA;QACA;QACAjB,aAAA;QACAA,aAAA;QACAA,aAAA;QAEAc,UAAA,CAAAO,GAAA;MACA;QACAP,UAAA,CAAAO,GAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAtB,aAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}