{"ast":null,"code":"import { extend } from \"vee-validate\";\nimport { required, numeric, regex, confirmed } from \"vee-validate/dist/rules\";\nextend(\"required\", required);\nextend(\"numeric\", numeric);\nextend(\"regex\", regex);\nextend(\"confirmed\", confirmed);\nexport default {\n  data: function data() {\n    return {\n      name: \"\",\n      address: \"\",\n      cost: \"\",\n      user: \"\"\n    };\n  },\n  methods: {\n    submit: function submit() {\n      alert(\"Form has been submitted!\");\n    }\n  }\n};","map":{"version":3,"names":["extend","required","numeric","regex","confirmed","data","name","address","cost","user","methods","submit","alert"],"sources":["src/pages/Residential/ResidentialForm.vue"],"sourcesContent":["<template>\n  <div class=\"col-md-12\">\n    <ValidationObserver v-slot=\"{ handleSubmit }\">\n      <form class=\"form-horizontal\" @submit.prevent=\"handleSubmit(submit)\">\n        <card>\n          <h4 slot=\"header\" class=\"card-title\">Type Validation</h4>\n          <div>\n            <div class=\"row\">\n              <label class=\"col-sm-2 col-form-label\">Requred Text</label>\n              <div class=\"col-sm-7\">\n                <ValidationProvider\n                  name=\"name\"\n                  rules=\"required\"\n                  v-slot=\"{ passed, failed, errors }\"\n                >\n                  <base-input\n                    required\n                    v-model=\"name\"\n                    :error=\"errors[0]\"\n                    :class=\"[\n                      { 'has-success': passed },\n                      { 'has-danger': failed },\n                    ]\"\n                  >\n                  </base-input>\n                </ValidationProvider>\n              </div>\n            </div>\n\n            <div class=\"row\">\n              <label class=\"col-sm-2 col-form-label\">Email</label>\n              <div class=\"col-sm-7\">\n                <ValidationProvider\n                  name=