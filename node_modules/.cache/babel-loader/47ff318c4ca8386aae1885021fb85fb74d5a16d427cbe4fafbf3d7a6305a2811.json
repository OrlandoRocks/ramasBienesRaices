   this.initScrollbar();\n  }\n};","map":{"version":3,"names":["PerfectScrollbar","SidebarShare","hasElement","className","document","getElementsByClassName","length","initScrollbar","concat","setTimeout","DashboardNavbar","ContentFooter","SidebarFixedToggleButton","ZoomCenterTransition","components","data","sidebarBackground","methods","toggleSidebar","$sidebar","showSidebar","displaySidebar","docClasses","body","classList","isWindows","navigator","platform","startsWith","add","mounted"],"sources":["src/pages/Layout/DashboardLayout.vue"],"sourcesContent":["<template>\n  <div class=\"wrapper\" :class=\"{ 'nav-open': $sidebar.showSidebar }\">\n    <notifications></notifications>\n    <sidebar-fixed-toggle-button />\n    <side-bar\n      :background-color=\"sidebarBackground\"\n      :short-title=\"$t('sidebar.shortTitle')\"\n      :title=\"$t('sidebar.title')\"\n    >\n      <template slot=\"links\">\n        <sidebar-item\n          :link=\"{\n            name: $t('sidebar.dashboard'),\n            icon: 'tim-icons icon-chart-pie-36',\n            path: '/dashboard',\n          }\"\n        >\n        </sidebar-item>\n        <sidebar-item\n          :link=\"{ name: $t('sidebar.pages'), icon: 'tim-icons icon-image-02' }\"\n        >\n          <sidebar-item\n            :link=\"{ name: $t('sidebar.timeline'), path: '/pages/timeline' }\"\n          ></sidebar-item>\n          <sidebar-item\n            :link=\"{ name: $t('sidebar.login'), path: '/login' }\"\n          ></sidebar-item>\n          <sidebar-item\n            :link=\"{ name: $t('sidebar.register'), path: '/register' }\"\n          ></sidebar-item>\n          <sidebar-item\n            :link=\"{ name: $t('sidebar.lock'), path: '/lock' }\"\n          ></sidebar-item>\n          <sidebar-item\n            :link=\"{ name: $t('sidebar.userProfile'), path: '/pages/user' }\"\n          ></sidebar-item>\n        </sidebar-item>\n        <sidebar-item\n          :link=\"{\n            name: $t('sidebar.widgets'),