 {\n      document.body.classList.remove(\"nav-open\");\n      this.showMenu = false;\n    },\n    setPageClass() {\n      this.pageClass = `${this.$route.name}-page`.toLowerCase();\n    },\n  },\n  beforeDestroy() {\n    this.closeMenu();\n  },\n  beforeRouteUpdate(to, from, next) {\n    // Close the mobile menu first then transition to next page\n    if (this.showMenu) {\n      this.closeMenu();\n      setTimeout(() => {\n        next();\n      }, this.menuTransitionDuration);\n    } else {\n      next();\n    }\n  },\n  mounted() {\n    this.setPageClass();\n  },\n  watch: {\n    $route() {\n      this.setPageClass();\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n.navbar.auth-navbar {\n  top: 0;\n}\n\n$scaleSize: 0.8;\n@keyframes zoomIn8 {\n  from {\n    opacity: 0;\n    transform: scale3d($scaleSize, $scaleSize, $scaleSize);\n  }\n  100% {\n    opacity: 1;\n  }\n}\n\n.wrapper-full-page .zoomIn {\n  animation-name: zoomIn8;\n}\n\n@keyframes zoomOut8 {\n  from {\n    opacity: 1;\n  }\n  to {\n    opacity: 0;\n    transform: scale3d($scaleSize, $scaleSize, $scaleSize);\n  }\n}\n\n.wrapper-full-page .zoomOut {\n  animation-name: zoomOut8;\n}\n</style>\n"],"mappings":";AA8FA,SAAAA,OAAA;AACA,SAAAC,oBAAA;AAEA;EACAC,UAAA;IACAF,OAAA,EAAAA,OAAA;IACAC,oBAAA,EAAAA;EACA;EACAE,KAAA;IACAC,eAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,OAAA;IACA;EACA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,QAAA;MACAC,sBAAA;MACAC,sBAAA;MACAC,IAAA,MAAAC,IAAA,GAAAC,WAAA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACAC,KAAA,WAAAA,MAAA;MACA,UAAAC,MAAA,MAAAC,MAAA,CAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,YAAA,WAAAA,aAAA;MACAC,QAAA,CAAAC,IAAA,CAAAC,SAAA,CAAAC,MAAA;MACA,KAAAjB,QAAA,SAAAA,QAAA;IACA;IACAkB,SAAA,WAAAA,UAAA;MACAJ,QAAA,CAAAC,IAAA,CAAAC,SAAA,CAAAG,MAAA;MACA,KAAAnB,QAAA;IACA;IACAoB,YAAA,WAAAA,aAAA;MACA,KAAAd,SAAA,MAAAG,MAAA,MAAAC,MAAA,CAAAC,IAAA,WAAAU,WAAA;IACA;EACA;EACAC,aAAA,WAAAA,cAAA;IACA,KAAAJ,SAAA;EACA;EACAK,iBAAA,WAAAA,kBAAAC,EAAA,EAAAC,IAAA,EAAAC,IAAA;IACA;IACA,SAAA1B,QAAA;MACA,KAAAkB,SAAA;MACAS,UAAA;QACAD,IAAA;M